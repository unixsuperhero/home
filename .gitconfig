# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
name = Joshua Toyota
email = jearsh@gmail.com
[init]
  defaultBranch = main
[core]
  whitespace = trailing-space,tab-in-indent,cr-at-eol,blank-at-eol,blank-at-eof
  fsmonitor = true
[diff]
  algorithm = histogram
  colorMoved = plain
  mnemonicPrefix = true
  renames = true
[push]
  default = simple
  autoSetupRemote = true
[pull]
  rebase = true
[rebase]
  autoSquash = true
  autoStash = true
  updateRefs = true
[merge]
  conflictstyle = diff3
[merge]
  stat = true
[credential]
  helper = osxkeychain
[url "git@github.com:"]
  insteadOf = https://github.com/
[feature]
  manyFiles = true

[alias]
  aa = add --all
  cb = branch --show-current
  conflicts = diff development --name-only --diff-filter=U
